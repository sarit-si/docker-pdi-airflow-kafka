    <py_script># Python Kafka Consumer

from kafka import KafkaConsumer, TopicPartition

BOOTSTRAP_SERVERS = '${BOOTSTRAP_SERVERS}'
GROUP_ID = '${group_id}'
TOPIC = '${topic}'
PARTITION = ${partition}

consumer = KafkaConsumer(
    bootstrap_servers=BOOTSTRAP_SERVERS,
    group_id=GROUP_ID,
    auto_offset_reset='earliest',
    enable_auto_commit=False,
    consumer_timeout_ms=6000,
    key_deserializer=lambda x : x.decode(),
    value_deserializer=lambda x : x.decode(),
)

consumer.assign([TopicPartition(TOPIC, PARTITION)])

message_values = list()

for record in consumer:
    message_values.append(str(record.offset) + '|' + record.value)

messages = ";".join(message_values)</py_script>
